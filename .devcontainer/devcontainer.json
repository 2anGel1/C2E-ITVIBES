{
    "name": "Laravel",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "bmewburn.vscode-intelephense-client"
            ],
            "settings": {
                "php.validate.enable": true,
                "php.csfixit.executablePath": "/usr/local/bin/php-cs-fixer",
                "php.csfixit.fixOnSave": true,
                "php.suggest.basic": false,
                "php.suggest.complete": false,
                "php.suggest.importDocBlocks": false,
                "php.suggest.shortenAliases": false,
                "php.suggest.showBuiltInSymbols": false,
                "php.suggest.showConstants": false,
                "php.suggest.showCustomSymbols": false,
                "php.suggest.showDeprecated": false,
                "php.suggest.showExtensions": false,
                "php.suggest.showKeywords": false,
                "php.suggest.showOperators": false,
                "php.suggest.showSnippets": false,
                "php.suggest.showVariables": false,
                "php.validate.run": "onSave"
            }
        }
    }
}